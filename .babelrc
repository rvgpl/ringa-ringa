{
  "env": {
    "test": {
      "presets": ["env", "react"],
      "plugins": ["transform-class-properties"],
      "ignore": ["node_modules/**"]
    },
    "development": {
      "presets": [["env", { "modules": false }], "react"],
      "plugins": ["transform-class-properties"],
      "ignore": ["node_modules/**"]
    },
    "production": {
      "presets": [["env", { "modules": false }], "react"],
      "plugins": ["transform-class-properties"],
      "ignore": ["node_modules/**"]
    }
  }
}
